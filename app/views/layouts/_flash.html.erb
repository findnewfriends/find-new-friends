<% if flash[:success] %>
  <div class="alert alert-success navbar mb-0" >
    <%= flash[:success] %>
  </div>
<% end %>

<% if flash[:danger] %>
  <div class="alert alert-danger navbar mb-0">
    <%= flash[:danger] %>
  </div>
<% end %>

<% if flash[:notice] %>
  <div class="alert alert-success mb-0 ">
    <%= flash[:notice] %>
  </div>
<% end %>

<% if flash[:alert] %>
  <div class="alert alert-warning mb-0">
    <%= flash[:alert] %>
  </div>
<% end %>

<% if flash[:error] %>
  <div class="alert alert-danger mb-0">
    <%= flash[:error] %>
  </div>
<% end %>